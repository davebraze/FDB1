citHeader("To cite FDB1 use:")
citEntry(entry = "Manual",
         title = "FDB1: a dubious brew of R functions",
         note = paste("R package version", packageVersion("FDB1")),
         author = personList(as.person("David Braze")),
         institution = "Haskins Laboratories",
         year = sub("-.*", "", meta$Date),
         url = "https://github.com/davebraze/FDB1",
         textVersion = "")
